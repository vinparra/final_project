<div class="page-header">
  <h3>Eligibility #<%= @eligibility.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Player</dt>
      <dd><%= @eligibility.player_id %></dd>

      <dt>Position</dt>
      <dd><%= @eligibility.position_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/eligibilities" class="btn btn-primary">
        Back
      </a>
      <a href="/eligibilities/<%= @eligibility.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_eligibility/<%= @eligibility.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
