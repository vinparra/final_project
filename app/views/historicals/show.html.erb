<div class="page-header">
  <h3>Historical #<%= @historical.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Player</dt>
      <dd>
        <% if @historical.player.present? %>
          <a href="/players/<%= @historical.player_id %>">
            <%= @historical.player.name %>
          </a>
        <% end %>
      </dd>

      <dt>Source</dt>
      <dd><%= @historical.source %></dd>

      <dt>Year</dt>
      <dd><%= @historical.year %></dd>

      <dt>At bats</dt>
      <dd><%= @historical.at_bats %></dd>

      <dt>Home runs</dt>
      <dd><%= @historical.home_runs %></dd>

      <dt>Runs batted in</dt>
      <dd><%= @historical.runs_batted_in %></dd>

      <dt>Stolen bases</dt>
      <dd><%= @historical.stolen_bases %></dd>

      <dt>Runs</dt>
      <dd><%= @historical.runs %></dd>

      <dt>Avg</dt>
      <dd><%= @historical.avg %></dd>

      <dt>Innings pitched</dt>
      <dd><%= @historical.innings_pitched %></dd>

      <dt>Wins</dt>
      <dd><%= @historical.wins %></dd>

      <dt>Saves</dt>
      <dd><%= @historical.saves %></dd>

      <dt>Era</dt>
      <dd><%= @historical.era %></dd>

      <dt>Whip</dt>
      <dd><%= @historical.whip %></dd>

      <dt>Strikeouts</dt>
      <dd><%= @historical.strikeouts %></dd>

      <dt>Hits</dt>
      <dd><%= @historical.hits %></dd>

      <dt>On base percentage</dt>
      <dd><%= @historical.on_base_percentage %></dd>

      <dt>Slugging percentage</dt>
      <dd><%= @historical.slugging_percentage %></dd>

      <dt>On base plus slugging</dt>
      <dd><%= @historical.on_base_plus_slugging %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/historicals" class="btn btn-primary">
        Back
      </a>
      <a href="/historicals/<%= @historical.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_historical/<%= @historical.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
